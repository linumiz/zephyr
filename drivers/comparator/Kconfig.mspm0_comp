config COMPARATOR_MSPM0
	bool "TI MSPM0 Comparator driver"
	default y
	depends on DT_HAS_TI_MSPM0_COMPARATOR_ENABLED
	select PINCTRL
	select SHARED_INTERRUPTS
	help
	  Enable driver for the Texas Instruments MSPM0 series comparators.
	  Supports reference voltage configuration and internal DAC control.
