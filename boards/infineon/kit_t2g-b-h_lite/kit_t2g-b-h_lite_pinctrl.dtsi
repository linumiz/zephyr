		pinmux = <DT_CAT1_PINMUX(7, 3, HSIOM_SEL_ACT_11)>;
		drive-push-pull;
		input-enable;
	};
	/omit-if-no-ref/ p7_4_smif0_data3: p7_4_smif0_data3 {
		pinmux = <DT_CAT1_PINMUX(7, 4, HSIOM_SEL_ACT_11)>;
		drive-push-pull;
		input-enable;
