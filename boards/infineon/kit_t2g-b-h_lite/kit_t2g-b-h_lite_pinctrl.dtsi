	/omit-if-no-ref/ p18_5_eth_txd_1: p18_5_eth_txd_1 {
		pinmux = <DT_CAT1_PINMUX(18, 5, HSIOM_SEL_ACT_12)>;
		drive-push-pull;
	};
	/omit-if-no-ref/ p18_1_eth_tx_en: p18_1_eth_tx_en {
		pinmux = <DT_CAT1_PINMUX(18, 1, HSIOM_SEL_ACT_12)>;
		drive-push-pull;
