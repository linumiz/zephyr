#include <arm/infineon/cat1c/traveo2-8m/cyt4bf8cds.dtsi>
#include <zephyr/dt-bindings/input/input-event-codes.h>
#include <zephyr/dt-bindings/pinctrl/ifx_cat1-pinctrl.h>
