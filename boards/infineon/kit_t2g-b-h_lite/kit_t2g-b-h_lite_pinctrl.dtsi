	/omit-if-no-ref/ p19_1_eth_rxd_1: p19_1_eth_rxd_1 {
		pinmux = <DT_CAT1_PINMUX(19, 1, HSIOM_SEL_ACT_12)>;
		input-enable;
	};
	/omit-if-no-ref/ p21_5_eth_rx_ctl: p21_5_eth_rx_ctl {
