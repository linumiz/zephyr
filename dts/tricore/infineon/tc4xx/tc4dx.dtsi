    cpus {
        #address-cells = <1>;
        #size-cells = <0>;

        cpu0: cpu@0 {
            compatible = "infineon,tc18";
            reg = <0>;
            clocks = <&fcpu0>;
        };
        cpu1: cpu@1 {
            compatible = "infineon,tc18";
            reg = <1>;
            clocks = <&fcpu1>;
        };
        cpu2: cpu@2 {
            compatible = "infineon,tc18";
            reg = <2>;
            clocks = <&fcpu2>;
        };
        cpu3: cpu@3 {
            compatible = "infineon,tc18";
