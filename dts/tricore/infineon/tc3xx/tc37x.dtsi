			uart9: uart@f0000f00 {
				compatible = "infineon,asclin-uart";
				reg = <0xF0000F00 256>;
				interrupts = <47 2>, <48 3>, <49 4>;
				interrupt-names = "tx", "rx", "err";
				clocks = <&ccu CLOCK_FASCLINF>, <&ccu CLOCK_FASCLINS>;
