	soc {
		#address-cells = <1>;
		#size-cells = <1>;
		interrupt-parent = <&ir>;
		compatible = "simple-bus";
		ranges;

		stm0: timer@f0001000 {
			compatible = "infineon,tc3xx-stm";
			reg = <0xF0001000 256>;
			clocks = <&fstm>;
			interrupts = <192 1>;
		};

		stm1: timer@f0001100 {
			compatible = "infineon,tc3xx-stm";
			reg = <0xF0001100 256>;
			clocks = <&fstm>;
			interrupts = <194 1>;
		};

		stm2: timer@f0001200 {
			compatible = "infineon,tc3xx-stm";
			reg = <0xF0001200 256>;
			clocks = <&fstm>;
			interrupts = <196 1>;
		};

