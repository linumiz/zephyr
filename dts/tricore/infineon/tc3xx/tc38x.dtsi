#include <infineon/tc3xx/tc38x-irq.h>

/ {
	model = "infineon,tc38x";
	compatible = "infineon,tc38x";

	cpus {
		#address-cells = <1>;
		#size-cells = <0>;

		cpu0: cpu@0 {
			compatible = "infineon,tc162";
			reg = <0>;
			clocks = <&fcpu0>;
		};
		cpu1: cpu@1 {
			compatible = "infineon,tc162";
			reg = <1>;
			clocks = <&fcpu1>;
		};
		cpu2: cpu@2 {
			compatible = "infineon,tc162";
			reg = <2>;
			clocks = <&fcpu2>;
		};
		cpu3: cpu@3 {
			compatible = "infineon,tc162";
			reg = <3>;
			clocks = <&fcpu3>;
		};
	};

	flash_controller: flash_controller@A8080000 {
		compatible = "infineon,tc3xx-flash-controller";
		#address-cells = <1>;
		#size-cells = <1>;
		reg = <0xA8080000 0x100000>;
		flash0: flash@80000000 {
			compatible = "infineon,tc3xx-nv-flash","soc-nv-flash";
			write-block-size = <256>;
			reg = <0x80000000 DT_SIZE_M(2)>;
		};
	};
	flash_controller1: flash_controller@A8380000 {
		compatible = "infineon,tc3xx-flash-controller";
		reg = <0xA8380000 0x100000>;
		#address-cells = <1>;
		#size-cells = <1>;
		flash1: flash@80300000 {
			compatible = "infineon,tc3xx-nv-flash","soc-nv-flash";
			write-block-size = <256>;
			reg = <0x80300000 DT_SIZE_M(2)>;
		};
	};
	flash_controller2: flash_controller@A8680000 {
		compatible = "infineon,tc3xx-flash-controller";
		reg = <0xA8680000 0x100000>;
		#address-cells = <1>;
		#size-cells = <1>;
		flash1: flash@80600000 {
			compatible = "infineon,tc3xx-nv-flash","soc-nv-flash";
			write-block-size = <256>;
			reg = <0x80600000 DT_SIZE_M(2)>;
		};
	};
	flash_controller3: flash_controller@A8980000 {
		compatible = "infineon,tc3xx-flash-controller";
		reg = <0xA8980000 0x100000>;
		#address-cells = <1>;
		#size-cells = <1>;
		flash1: flash@80900000 {
