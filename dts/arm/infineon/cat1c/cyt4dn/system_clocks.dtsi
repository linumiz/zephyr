#define DIV_8_BIT	00
#define DIV_16_BIT	01
#define DIV_16_5_BIT	02
#define DIV_24_5_BIT	03

#include <dt-bindings/clock/ifx_clock_source_def.h>

/ {
	clocks {
		/* ilo */
		clk_ilo: clk_ilo {
			#clock-cells = <0>;
			compatible = "infineon,fixed-clock";
			clock-frequency = <32768>;
			clock-block = <7>; /* ILO */
			status = "okay";
		};

		/* eco */
		clk_eco: clk_eco {
			#clock-cells = <0>;
			compatible = "infineon,fixed-clock";
			clock-frequency = <16000000>;
			clock-block = <2>; /* ECO */
			status = "okay";
		};

		/* imo */
		clk_imo: clk_imo {
			#clock-cells = <0>;
			compatible = "fixed-clock";
			clock-frequency = <8000000>;
			status = "disabled";
		};

		/* ext_clk */
		clk_ext: clk_ext {
			#clock-cells = <0>;
			compatible = "fixed-clock";
			status = "disabled";
		};

		/* fll */
		fll0: fll0 {
			#clock-cells = <0>;
			compatible = "fixed-clock";
			clock-frequency = <96000000>;
			status = "disabled";
		};

		pll0: pll0 {
			#clock-cells = <0>;
			compatible = "infineon,cat1-pll";
			clocks = <&clk_eco>;
			ifx,pll-instance = <1>;
			p-div = <40>;
			q-div = <1>;
			output-div = <2>;
			status = "disabled";
		};

		pll1: pll1 {
			#clock-cells = <0>;
			compatible = "infineon,cat1-pll";
			clocks = <&clk_eco>;
			ifx,pll-instance = <2>;
			p-div = <31>;
			q-div = <1>;
			output-div = <2>;
			status = "disabled";
		};

		pll2: pll2 {
			#clock-cells = <0>;
			compatible = "infineon,cat1-pll";
			clocks = <&clk_eco>;
			ifx,pll-instance = <3>;
			p-div = <31>;
			q-div = <1>;
			output-div = <2>;
			status = "disabled";
		};

		pll3: pll3 {
			#clock-cells = <0>;
			compatible = "infineon,cat1-pll";
			clocks = <&clk_eco>;
			ifx,pll-instance = <4>;
			p-div = <31>;
			q-div = <1>;
			output-div = <2>;
			status = "disabled";
		};

		pll4: pll4 {
			#clock-cells = <0>;
			compatible = "infineon,cat1-pll";
			clocks = <&clk_eco>;
			ifx,pll-instance = <5>;
			p-div = <62>;
			q-div = <2>;
			output-div = <2>;
			status = "disabled";
		};

		pll5: pll5 {
			#clock-cells = <0>;
			compatible = "infineon,cat1-pll";
			clocks = <&clk_eco>;
			ifx,pll-instance = <6>;
			p-div = <30>;
			q-div = <2>;
			output-div = <2>;
			status = "disabled";
		};

		pll6: pll6 {
			#clock-cells = <0>;
			compatible = "infineon,cat1-pll";
			clocks = <&clk_eco>;
			ifx,pll-instance = <7>;
			p-div = <25>;
			q-div = <2>;
			output-div = <2>;
			status = "disabled";
		};

		pll7: pll7 {
			#clock-cells = <0>;
			compatible = "infineon,cat1-pll";
			clocks = <&clk_eco>;
			ifx,pll-instance = <8>;
			p-div = <47>;
			q-div = <3>;
			output-div = <2>;
			status = "disabled";
		};

		/* clk_hf0 */
		clk_hf0: clk_hf0 {
			#clock-cells = <0>;
			compatible = "infineon,cat1-clock";
			clock-frequency = <125000000>;
			ifx,hf-predivider = <IFX_CAT1_CLKHF_NO_DIVIDE>;
			ifx,hf-instance = <0>;
			clocks = <&pll5>;
			status = "disabled";
		};

		/* clk_hf1 */
		clk_hf1: clk_hf1 {
			#clock-cells = <0>;
			compatible = "infineon,cat1-clock";
			clock-frequency = <320000000>;
			ifx,hf-predivider = <IFX_CAT1_CLKHF_NO_DIVIDE>;
			clocks = <&pll0>;
			ifx,hf-instance = <1>;
		};

		/* clk_hf2 */
		clk_hf2: clk_hf2 {
			#clock-cells = <0>;
			compatible = "infineon,cat1-clock";
			clock-frequency = <100000000>;
			ifx,hf-predivider = <IFX_CAT1_CLKHF_NO_DIVIDE>;
			ifx,hf-instance = <2>;
			clocks = <&pll6>;
		};

		/* clk_hf3 */
		clk_hf3: clk_hf3 {
			#clock-cells = <0>;
			compatible = "infineon,cat1-clock";
			clock-frequency = <196000000>;
			ifx,hf-predivider = <IFX_CAT1_CLKHF_DIVIDE_BY_2>;
			clocks = <&pll5>;
			ifx,hf-instance = <3>;
		};

		/* clk_hf4 */
		clk_hf4: clk_hf4 {
			#clock-cells = <0>;
			compatible = "infineon,cat1-clock";
			clock-frequency = <125000000>;
			ifx,hf-predivider = <IFX_CAT1_CLKHF_NO_DIVIDE>;
			clocks = <&pll7>;
			ifx,hf-instance = <4>;
		};

		/* clk_hf5 */
		clk_hf5: clk_hf5 {
			#clock-cells = <0>;
			compatible = "infineon,cat1-clock";
			clock-frequency = <196000000>;
			ifx,hf-predivider = <IFX_CAT1_CLKHF_NO_DIVIDE>;
			clocks = <&pll1>;
			ifx,hf-instance = <5>;
		};

		/* clk_hf6 */
		clk_hf6: clk_hf6 {
			#clock-cells = <0>;
			compatible = "infineon,cat1-clock";
			clock-frequency = <196000000>;
			ifx,hf-predivider = <IFX_CAT1_CLKHF_NO_DIVIDE>;
			clocks = <&pll1>;
			ifx,hf-instance = <6>;
		};

		/* clk_hf7 */
		clk_hf7: clk_hf7 {
			#clock-cells = <0>;
			compatible = "infineon,cat1-clock";
			clock-frequency = <196000000>;
			ifx,hf-predivider = <IFX_CAT1_CLKHF_NO_DIVIDE>;
			ifx,hf-instance = <7>;
			clocks = <&pll1>;
		};

		/* clk_hf8 */
		clk_hf8: clk_hf8 {
			#clock-cells = <0>;
			compatible = "infineon,cat1-clock";
			clock-frequency = <320000000>;
			ifx,hf-predivider = <IFX_CAT1_CLKHF_NO_DIVIDE>;
			ifx,hf-instance = <8>;
			clocks = <&pll2>;
		};

		/* clk_hf9 */
		clk_hf9: clk_hf9 {
			#clock-cells = <0>;
			compatible = "infineon,cat1-clock";
			clock-frequency = <320000000>;
			ifx,hf-predivider = <IFX_CAT1_CLKHF_NO_DIVIDE>;
			ifx,hf-instance = <9>;
			clocks = <&pll3>;
		};

		/* clk_hf10 */
		clk_hf10: clk_hf10 {
			#clock-cells = <0>;
			compatible = "infineon,cat1-clock";
			clock-frequency = <250000000>;
			ifx,hf-predivider = <IFX_CAT1_CLKHF_NO_DIVIDE>;
			ifx,hf-instance = <10>;
			clocks = <&pll3>;
		};

		/* clk_hf11 */
		clk_hf11: clk_hf11 {
			#clock-cells = <0>;
			compatible = "infineon,cat1-clock";
			clock-frequency = <220000000>;
			ifx,hf-predivider = <IFX_CAT1_CLKHF_NO_DIVIDE>;
			ifx,hf-instance = <11>;
			clocks = <&pll4>;
		};

		/* clk_hf12 */
		clk_hf12: clk_hf12 {
			#clock-cells = <0>;
			compatible = "infineon,cat1-clock";
			clock-frequency = <200000000>;
			ifx,hf-predivider = <IFX_CAT1_CLKHF_NO_DIVIDE>;
			ifx,hf-instance = <12>;
			clocks = <&pll7>;
		};

		/* clk_hf13 */
		clk_hf13: clk_hf13 {
			#clock-cells = <0>;
			compatible = "infineon,cat1-clock";
			clock-frequency = <200000000>;
			ifx,hf-predivider = <IFX_CAT1_CLKHF_NO_DIVIDE>;
			ifx,hf-instance = <13>;
		};
	};
};
